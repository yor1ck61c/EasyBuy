<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.oicp.yorick61c.mapper.UserMapper">

    <!--sql抽取-->
    <sql id="selectUser">select * from user</sql>

    <!--增-->
    <insert id="register" parameterType="io.oicp.yorick61c.domain.EbUser">
        insert into eb_user value (#{euUserId},#{euUserName},#{euPassword},#{euSex},
        #{euBirthday},#{euIdentityCode},#{euEmail,#{euMobile},#{euAddress},#{euStatus})
    </insert>
    <!--删-->

    <!--改-->

    <!--查-->
    <select id="findAll" resultType="user">
        select * from user
    </select>


</mapper>